<Page x:Class="Tournament_421_HabibullinDmitry.Pages.ReportPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Tournament_421_HabibullinDmitry.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ReportPage">

    <Grid>
        <StackPanel Orientation="Horizontal" Grid.ColumnSpan="2" HorizontalAlignment="Center" Margin="0,300,0,0">
            <Button Width="200" Height="50" x:Name="EndToBtn" Content="ЗАВЕРШЁННЫЕ ТУРНИРЫ" Grid.ColumnSpan="2" Margin="0,0,30,0" Click="EndToBtn_Click"></Button>
            <Button Width="200" Height="50" x:Name="FutureToBtn" Content="ПРЕДСТОЯЩИЕ ТУРНИРЫ" Grid.ColumnSpan="2" Click="FutureToBtn_Click"></Button>
        </StackPanel>
    </Grid>
</Page>
