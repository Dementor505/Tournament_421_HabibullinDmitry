<UserControl x:Class="Tournament_421_HabibullinDmitry.ControlsWrap.TournamentControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Tournament_421_HabibullinDmitry.ControlsWrap"
             mc:Ignorable="d" 
             d:DesignHeight="110" d:DesignWidth="1200">
    <Grid Height="110" Width="1200">
            <Border BorderBrush="Black" BorderThickness="2">
        <StackPanel HorizontalAlignment="Center">
            <TextBlock x:Name="NameT" FontSize="20">FFFFF</TextBlock>
            <TextBlock x:Name="StartT" FontSize="15">FFFFF</TextBlock>
            <TextBlock x:Name="EndT" FontSize="15">FFFFF</TextBlock>
            <TextBlock x:Name="MoneyT" FontSize="15">FFFFF</TextBlock>
            <TextBlock x:Name="PlayersT" FontSize="15">FFFFF</TextBlock>
        </StackPanel>
            </Border>
    </Grid>
</UserControl>
